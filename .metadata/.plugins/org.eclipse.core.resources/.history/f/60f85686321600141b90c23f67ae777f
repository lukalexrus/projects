		// String s="";
		 int[]arr={4,5,6,7,8,9,0};
	        for(int i:arr)
	        {
	            System.out.println(i);
	        }
	        int i=0;
	        while(i++<arr.length-1){
	        	System.out.println(arr[i]+" "+i);
	        }
	
	}

