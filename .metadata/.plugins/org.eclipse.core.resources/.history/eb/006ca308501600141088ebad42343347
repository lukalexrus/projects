package Test;

import java.awt.event.KeyAdapter;
import java.awt.event.KeyEvent;
import java.io.BufferedReader;
import java.io.IOException;
import java.io.InputStreamReader;
import java.util.Scanner;
import java.util.regex.Pattern;

public class Progamm {

	
	public static void clear()
	{
		for (int i=0; i<25; i++)
		    System.out.println();
	}
	
	
	public static void menu() throws IOException
	{
		boolean check=true;
		 clear();
	 while (check)
	 {
		
	    System.out.println("МЕНЮ");
		System.out.println("1.Embeded class");
		System.out.println("2.Track time");
		System.out.println("3.Exit");
	   
		BufferedReader input = new BufferedReader(new InputStreamReader(System.in));
		String str=input.readLine();  // переменная для ответа.
		switch(str)
		{
			case "1":
				{
					System.out.println("1 known command");
					check=false;
					break;					
				}
			case "2":
				{
					System.out.println("2 known command");
					check=false;
					break;
				}
			case "3":
				{
					System.out.println("3 known command");
					check=false;
					break;
				}
				default:
				{
					System.out.println("Unknown command");
				}
		}
		
		//System.out.println(str);
	 }
	
	}




	public static void main(String[] args) throws IOException
	{
		menu();
	}
	
	
}
